{
  "name": "onion-root",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "pnpm --filter onion-interceptor test",
    "dev": "pnpm --filter @onion-interceptor/example dev",
    "build": "pnpm --filter onion-interceptor build",
    "build:watch": "pnpm --filter onion-interceptor build:watch",
    "docs:dev": "concurrently \"typedoc --watch\" \"vitepress dev docs\"",
    "docs:build": "typedoc && vitepress build docs",
    "docs:preview": "typedoc && vitepress preview docs",
    "pipes:build": "pnpm --filter @onion-interceptor/pipes build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ignore-pattern .gitignore .",
    "changed": "dotenv lerna version --conventional-commits --create-release github --yes",
    "changed:patch": "dotenv lerna version patch --conventional-commits --create-release github --yes",
    "changed:major": "dotenv lerna version major --conventional-commits --create-release github --yes",
    "release": "lerna publish from-package --yes"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@lerna-lite/cli": "^3.7.1",
    "@lerna-lite/publish": "^3.7.1",
    "@lerna-lite/version": "^3.7.1",
    "@onion-interceptor/pipes": "workspace:*",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.14.12",
    "@vitest/coverage-v8": "^2.0.0",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "coveralls": "^3.1.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.8.0",
    "globals": "^15.8.0",
    "jsdom": "^22.1.0",
    "oh-my-live2d": "^0.16.1",
    "onion-interceptor": "workspace:*",
    "release-it": "^15.11.0",
    "rimraf": "^5.0.9",
    "terser": "^5.31.3",
    "typedoc": "0.25.13",
    "typedoc-plugin-markdown": "4.0.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.3.5",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vitepress": "^1.3.1",
    "vitest": "^2.0.0"
  },
  "coveralls": {
    "repo_token": "zr4ywJD56s71hjhyknPAMaw3mCUS4zbWD"
  }
}
