{
  "name": "@onion-interceptor/workspace",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @onion-interceptor/example dev",
    "build": "pnpm --filter onion-interceptor build",
    "build:watch": "pnpm --filter onion-interceptor build:watch",
    "docs:dev": "concurrently \"typedoc --watch\" \"vitepress dev docs\"",
    "docs:build": "typedoc && vitepress build docs",
    "docs:preview": "typedoc && vitepress preview docs"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@eslint/js": "^9.7.0",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.6.0",
    "@onion-interceptor/adapter": "workspace:*",
    "onion-interceptor": "workspace:*",
    "concurrently": "^8.2.2",
    "axios": "^1.7.2",
    "eslint": "^9.7.0",
    "jsdom": "^22.1.0",
    "oh-my-live2d": "^0.16.1",
    "prettier": "^3.0.0",
    "release-it": "^15.5.1",
    "rimraf": "^5.0.1",
    "typedoc": "0.25.13",
    "typedoc-plugin-markdown": "4.0.3",
    "typescript": "5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-dts": "4.0.0-beta.1",
    "vitepress": "^1.1.0",
    "vitest": "^1.6.0"
  }
}
